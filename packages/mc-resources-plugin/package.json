{
  "name": "@hato810424/mc-resources-plugin",
  "version": "0.0.0-beta.9",
  "type": "module",
  "author": "hato810424",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hato810424/mc-resources-plugin.git"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/hato810424"
  },
  "exports": {
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "default": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.cts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs",
      "default": "./dist/webpack.cjs"
    },
    "./docusaurus": {
      "types": "./dist/docusaurus.d.ts",
      "import": "./dist/docusaurus.mjs",
      "default": "./dist/docusaurus.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "concurrently -c cyan,yellow --names esm,cjs \"tsdown --watch --format esm\" \"tsdown --watch --format cjs\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@docusaurus/types": "^3.9.2",
    "@types/node": "^25.0.10",
    "concurrently": "^9.2.1",
    "tsdown": "^0.18.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.16",
    "webpack": "^5.104.1",
    "webpack-dev-server": "^5.2.3"
  },
  "dependencies": {
    "canvas": "^3.2.1",
    "chalk": "^5.6.2",
    "find-cache-directory": "^6.0.0",
    "node-stream-zip": "^1.15.0",
    "sharp": "^0.34.5",
    "zod": "^4.3.6"
  }
}
